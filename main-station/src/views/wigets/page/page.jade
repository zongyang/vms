mixin page()
    .page.text-center
        ul.pagination
            li
                a(href='#') &laquo;
            li
                a(href='#') 1
            li.active
                a(href='#') 2
            li
                a(href='#') 3
            li
                a(href='#') 4
            li
                a(href='#') &raquo;